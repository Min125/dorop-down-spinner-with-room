<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/nextFragment">

    <fragment
        android:id="@+id/nextFragment"
        android:name="com.letuse.spinnerwithlocalarrayfragment.nextFragment"
        android:label="fragment_next"
        tools:layout="@layout/fragment_next" >
        <action
            android:id="@+id/action_nextFragment_to_categoryFragment"
            app:destination="@id/categoryFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.letuse.spinnerwithlocalarrayfragment.CategoryFragment"
        android:label="fragment_category"
        tools:layout="@layout/fragment_category" />
</navigation>